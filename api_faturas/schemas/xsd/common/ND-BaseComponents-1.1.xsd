<xsd:schema xmlns="urn:netdocs:schemas:document" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msg="urn:netdocs:schemas:message" targetNamespace="urn:netdocs:schemas:document" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.1">
	<xsd:annotation>
		<xsd:documentation xml:lang="pt">
			Módulo com definições de tipos de dados comuns para diversos tipos de documentos. Este módulo é a base para qualquer documento implementado no Netdocs.
		</xsd:documentation>
	</xsd:annotation>
	<!-- ================== -->
	<!-- ===== Include   ===== -->
	<!-- ================== -->
	<xsd:include schemaLocation="D:/Python/Sage/Havas/api_faturas/schemas/xsd/common/ND-BasicDataTypes-1.1.xsd"/>
	<!-- ========================= -->
	<!-- ===== Includes & Imports   ===== -->
	<!-- ========================= -->
	<xsd:import namespace="urn:netdocs:schemas:message" schemaLocation="D:/Python/Sage/Havas/api_faturas/schemas/xsd/common/ND-BaseMessageComponents-1.1.xsd"/>
	<!-- ======================== -->
	<!-- =====  Complex Types ====== -->
	<!-- ======================== -->
	<xsd:complexType name="structuredDescriptionType">
		<xsd:annotation>
			<xsd:documentation>Tipo de descrições com identificação da descrição. Utilizar quando houver necessidade de identificar as descrições</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="descriptionType">
				<xsd:attribute name="id" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Usar este campo no caso de ser necessário identificar os comentários</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="registrationInformationType">
		<xsd:annotation>
			<xsd:documentation>Informação de registos das empresas em entidades reguladoras</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="entityId" type="nonEmptyString">
				<xsd:annotation>
					<xsd:documentation>Entidade reguladora à qual foi efetuado o registo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="registrationId" type="nonEmptyString">
				<xsd:annotation>
					<xsd:documentation>Identificador do registo atribuido</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itemCodeAssignedByRegulatorType">
		<xsd:annotation>
			<xsd:documentation>Código do artigo ou serviço atribuido por entidade reguladora</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="nonEmptyString">
				<xsd:attribute name="regulator" type="regulatorEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">Nome de entidade reguladora</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="regulatorCodeType" type="regulatorCodeEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">Tipo de código atribuído por entidade reguladora</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="referenceType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para referências</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="type" use="required">
					<xsd:annotation>
						<xsd:documentation>Tipo do documento referênciado</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="referenceEnumType"/>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="referencedDocumentDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Data do documento referênciado.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="referencedDocumentId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Número do documento referênciado.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="lineId" type="xsd:long">
					<xsd:annotation>
						<xsd:documentation>Identificador da linha referênciada no documento</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="documentReferenceType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para referênciar documentos</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="type" type="referenceEnumType">
					<xsd:annotation>
						<xsd:documentation>Tipo do documento referênciado</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="referencedDocumentDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Data do documento referênciado.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="referencedDocumentId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Número do documento referênciado.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="lineId" type="xsd:long">
					<xsd:annotation>
						<xsd:documentation>Identificador da linha referênciada no documento</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="detailedDocumentReferenceType">
		<xsd:annotation>
			<xsd:documentation>Referência detalhada a documentos, para além do número, tipo de data, contém também informação sobre que emitiu e recebeu.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="originalSender" type="nonBillableCompanyInformationType">
				<xsd:annotation>
					<xsd:documentation>Referencia à entidade emissora do documento original</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="originalReceiver" type="nonBillableCompanyInformationType">
				<xsd:annotation>
					<xsd:documentation>Referencia à entidade receptora do documento original</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="originalDocument" type="documentReferenceType" minOccurs="0"/>
			<xsd:element name="originalMessageId" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="documentAdditionalDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados, baseado no xsd:dateTime, que permite identificar o tipo de informação de data e hora.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:dateTime">
				<xsd:attribute name="type" type="documentDateEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation>Qualificador do tipo de data</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="documentLineAdditionalDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados, baseado no xsd:dateTime, que permite identificar o tipo de informação de data e hora.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:dateTime">
				<xsd:attribute name="type" type="documentLineDateEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation>Qualificador do tipo de data</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="deliveryRelatedDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados, baseado no xsd:dateTime, que permite identificar o tipo de informação de data e hora, relacionadas com a entrega.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:dateTime">
				<xsd:attribute name="type" type="deliveryRelatedDateEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation>Qualificador do tipo de data</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="baseLegalCompanyInformationType" abstract="true">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados base para definir entidades que podem ser facturadas ou emitir facturas, com a informação necessária para ter validade legal. Não deve ser utilizado directamente, deve antes ser utilizado em extensões ou restrições.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nonBillableCompanyInformationType">
				<xsd:sequence>
					<xsd:element name="vatNumber" type="vatNumberType">
						<xsd:annotation>
							<xsd:documentation>Número de indentificação fiscal (ou pessoa colectiva) da entidade.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="commercialRegistrationNumber" type="commercialRegistrationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Número do registo comercial.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="commercialRegistrationLocation" type="commercialRegistrationLocationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Conservatória do registo comercial onde a entidade está registada.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="socialCapital" type="socialCapitalType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Capital social.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="registrationInformation" type="registrationInformationType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Informação de registo em entidades reguladoras</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="billableCompanyInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para definir entidades que podem ser facturadas, com a informação necessária para ter validade legal</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="baseLegalCompanyInformationType">
				<xsd:sequence>
					<xsd:sequence>
						<xsd:element name="id" type="entityIdType" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Identificação da empresa.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="name" type="entityNameType">
							<xsd:annotation>
								<xsd:documentation>Nome da empresa</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="addressInformation" type="addressInformationType">
							<xsd:annotation>
								<xsd:documentation>Morada completa</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="contactInformation" type="contactInformationType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Contactos.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence minOccurs="0">
						<xsd:element name="subDivision" type="companySubDivisionInformationType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Divisões orgânicas da entidade</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element name="vatNumber" type="vatNumberType">
							<xsd:annotation>
								<xsd:documentation>Número de indentificação fiscal (ou pessoa colectiva) da entidade.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="commercialRegistrationNumber" type="commercialRegistrationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Número do registo comercial.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="commercialRegistrationLocation" type="commercialRegistrationLocationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Conservatória do registo comercial onde a entidade está registada.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="socialCapital" type="socialCapitalType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Capital social.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="registrationInformation" type="registrationInformationType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Informação de registo em entidades reguladoras</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="billingCompanyInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para definir entidades que podem emitir facturas, ou outro tipo de documento com requisitos legais, com a informação necessária para ter validade legal</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="billableCompanyInformationType">
				<xsd:sequence>
					<xsd:sequence>
						<xsd:element name="id" type="entityIdType" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Identificação da empresa.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="name" type="entityNameType">
							<xsd:annotation>
								<xsd:documentation>Nome da empresa</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="addressInformation" type="addressInformationType">
							<xsd:annotation>
								<xsd:documentation>Morada completa</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="contactInformation" type="contactInformationType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Contactos.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence minOccurs="0">
						<xsd:element name="subDivision" type="companySubDivisionInformationType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Divisões orgânicas da entidade</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element name="vatNumber" type="vatNumberType">
							<xsd:annotation>
								<xsd:documentation>Número de indentificação fiscal (ou pessoa colectiva) da entidade.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="commercialRegistrationNumber" type="commercialRegistrationType">
							<xsd:annotation>
								<xsd:documentation>Número do registo comercial.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="commercialRegistrationLocation" type="commercialRegistrationLocationType">
							<xsd:annotation>
								<xsd:documentation>Conservatória do registo comercial onde a entidade está registada.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="socialCapital" type="socialCapitalType">
							<xsd:annotation>
								<xsd:documentation>Capital social.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="registrationInformation" type="registrationInformationType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Informação de registo em entidades reguladoras</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:sequence>
			</xsd:restriction>
			<!--xsd:extension base="nonBillableCompanyInformationType">
				<xsd:group ref="billingCompanyInformationPartGroup"/>
			</xsd:extension-->
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="nonBillableCompanyInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para definir entidades que não necessitem de informação com caracter de legal</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="companyInformationType">
				<xsd:sequence minOccurs="0">
					<xsd:element name="subDivision" type="companySubDivisionInformationType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Divisões orgânicas da entidade</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="companySubDivisionInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para definir divisões organicas de uma entidade, por exemplo departamentos</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="companyInformationType">
				<xsd:attribute name="subDivisionType" type="companySubDivisionTypeEnum" use="required">
					<xsd:annotation>
						<xsd:documentation>Tipo de divisão orgânica da empresa</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="discountChargeInformationType" abstract="true">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados base para descontos e encargos. Não pode ser instânciado directamente.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="amount" type="nonNegativeDecimalType">
				<xsd:annotation>
					<xsd:documentation>Montante do desconto/encargo.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="percentage" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Montante do desconto/encargo.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxableAmount" type="nonNegativeDecimalType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Montante sujeito á taxa.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
            <xsd:element name="description" type="descriptionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Descrição do desconto ou encargo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
            <xsd:element name="baseAmount" type="nonNegativeDecimalType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Valor base sobre o qual o desconto/encargo incide.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
		<xsd:attribute name="applicationSequence" type="nonNegativeIntegerType">
			<xsd:annotation>
				<xsd:documentation>Sequencia de cálculo do desconto ou encargo</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="discountInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados base para descontos.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="discountChargeInformationType">
				<xsd:attribute name="type" type="discountTypeEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation>Tipo de desconto</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="chargeInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados base para encargos.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="discountChargeInformationType">
				<xsd:attribute name="type" type="chargeTypeEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation>Tipo de encargo</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="quantityType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para quantidades com unidades de medida.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="value" type="allDecimalType">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Valor da quantidade.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="unitOfMeasure" type="unitOfMeasureType">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Unidade de medida usada.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="stampDutyType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para o imposto de selo.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice minOccurs="0" maxOccurs="1">
				<xsd:element name="percentage" type="percentageType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">Percentagem</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="value" type="decimalType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">Valor</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="taxTotalValue" type="decimalType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Montante do imposto de selo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxableAmount" type="decimalType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Base de incidência do imposto de selo.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paragraph" type="nonEmptyString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Valor</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="withholdingTaxType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para a retenção na fonte.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="taxPercentage" type="percentageType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Percentagem</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxTotalValue" type="decimalType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Montante da retenção na fonte.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxableAmount" type="decimalType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Base de incidência da retenção na fonte.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Descritivo da retenção na fonte.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Tipo de Retenção na Fonte</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
