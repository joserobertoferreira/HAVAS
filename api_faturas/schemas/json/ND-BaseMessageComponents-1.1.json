{
  "@xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
  "@xmlns": "urn:netdocs:schemas:message",
  "@targetNamespace": "urn:netdocs:schemas:message",
  "@elementFormDefault": "unqualified",
  "@attributeFormDefault": "unqualified",
  "@version": "1.1",
  "@finalDefault": [],
  "@blockDefault": [],
  "xsd:annotation": [
    {
      "xsd:documentation": [
        {
          "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
          "$": "Módulo com definições de tipos de dados comuns ao nível das mensagens."
        }
      ]
    }
  ],
  "xsd:include": [
    {
      "@schemaLocation": "../common/ND-SharedBasicDataTypes_Message-1.1.xsd"
    }
  ],
  "xsd:element": {
    "@name": "genericDocument",
    "@type": "baseDocumentType",
    "@abstract": true,
    "@nillable": false,
    "xsd:annotation": {
      "xsd:documentation": [
        {
          "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
          "$": "Elemento abstracto para permitir o envio de documentos dentro do elemento message. Para isso é preciso criar elementos com type=\"baseDocumentType\" e com substitutionGroup=\"genericDocument\". Este elemento não pode ser instânciado, mas serve para indicar o tipo de conteúdo do elemento message."
        }
      ]
    }
  },
  "xsd:complexType": [
    {
      "@name": "messageType",
      "@abstract": false,
      "@mixed": false,
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "messageHeaderType",
          "xsd:sequence": {
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "xsd:element": [
              {
                "@ref": "genericDocument",
                "@maxOccurs": "unbounded",
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    {
                      "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                      "$": "Elemento que deve ser substituido pelo elemento raiz do documento a ser enviado"
                    }
                  ]
                }
              },
              {
                "@name": "extensions",
                "@type": "extensionType",
                "@minOccurs": 0,
                "@maxOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Dentro deste elemento pode ser enviadas estruturas XML válidas."
                  ]
                }
              }
            ]
          },
          "xsd:attribute": [
            {
              "@name": "id",
              "@type": "xsd:string",
              "@use": "required",
              "xsd:annotation": {
                "xsd:documentation": [
                  {
                    "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                    "$": "Código único, atribuido pelo emissor, que deve identificar univocamente cada mensagem"
                  }
                ]
              }
            },
            {
              "@name": "creationDateTime",
              "@type": "xsd:dateTime",
              "@use": "required",
              "xsd:annotation": {
                "xsd:documentation": [
                  {
                    "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                    "$": "Data e hora de criação da mensagem"
                  }
                ]
              }
            }
          ]
        }
      }
    },
    {
      "@name": "messageHeaderType",
      "@abstract": false,
      "@mixed": false,
      "xsd:sequence": {
        "@maxOccurs": 1,
        "@minOccurs": 1,
        "xsd:element": [
          {
            "@name": "sender",
            "@type": "companyInformationType",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Identificação da entidade emissora da mensagem"
                }
              ]
            }
          },
          {
            "@name": "receiver",
            "@type": "companyInformationType",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Identificação da entidade receptora da mensagem"
                }
              ]
            }
          }
        ]
      }
    }
  ]
}