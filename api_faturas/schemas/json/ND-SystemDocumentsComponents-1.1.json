{
  "@xmlns": "urn:netdocs:schemas:document",
  "@xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
  "@xmlns:msg": "urn:netdocs:schemas:message",
  "@targetNamespace": "urn:netdocs:schemas:document",
  "@elementFormDefault": "unqualified",
  "@attributeFormDefault": "unqualified",
  "@version": "1.1",
  "@finalDefault": [],
  "@blockDefault": [],
  "xsd:annotation": [
    {
      "xsd:documentation": [
        {
          "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
          "$": "Módulo com definições de tipos de dados para documentos utilizado em operações de gestão do sistem, como sejam a criação de utilizadore e empresas."
        }
      ]
    }
  ],
  "xsd:include": [
    {
      "@schemaLocation": "../common/ND-BaseComponents-1.1.xsd"
    }
  ],
  "xsd:complexType": [
    {
      "@name": "entityType",
      "@abstract": false,
      "@mixed": false,
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "documentType",
          "xsd:sequence": {
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "xsd:element": [
              {
                "@name": "id",
                "@type": "entityIdType",
                "@maxOccurs": "unbounded",
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Identificação da empresa."
                  ]
                }
              },
              {
                "@name": "name",
                "@type": "entityNameType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Nome da empresa"
                  ]
                }
              },
              {
                "@name": "nickName",
                "@type": "entityNickNameType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Nome abreviado da empresa"
                  ]
                }
              },
              {
                "@name": "addressInformation",
                "@type": "addressInformationType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Morada completa"
                  ]
                }
              },
              {
                "@name": "contactInformation",
                "@type": "contactInformationType",
                "@minOccurs": 0,
                "@maxOccurs": "unbounded",
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Contactos."
                  ]
                }
              },
              {
                "@name": "languageCode",
                "@type": "languageCodeTypeEnum",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Idioma da Entidade."
                  ]
                }
              },
              {
                "@name": "vatNumber",
                "@type": "vatNumberType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Número de indentificação fiscal (ou pessoa colectiva) da entidade."
                  ]
                }
              },
              {
                "@name": "commercialRegistrationNumber",
                "@type": "commercialRegistrationType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Número do registo comercial."
                  ]
                }
              },
              {
                "@name": "commercialRegistrationLocation",
                "@type": "commercialRegistrationLocationType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Conservatória do registo comercial onde a entidade está registada."
                  ]
                }
              },
              {
                "@name": "socialCapital",
                "@type": "socialCapitalType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Capital social."
                  ]
                }
              },
              {
                "@name": "userIdAssociation",
                "@type": "userIdType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Utilizador a associar à entidade."
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "@name": "userType",
      "@abstract": false,
      "@mixed": false,
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "documentType",
          "xsd:sequence": {
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "xsd:element": [
              {
                "@name": "id",
                "@type": "userIdType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Identificação do utilizador, utilizada para autenticação"
                  ]
                }
              },
              {
                "@name": "name",
                "@type": "userNameType",
                "@minOccurs": 0,
                "@maxOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Nome do Utilizador a ser criado"
                  ]
                }
              },
              {
                "@name": "passWord",
                "@type": "userPassWordType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Senha de autenticação do utilizador"
                  ]
                }
              },
              {
                "@name": "email",
                "@type": "emailContactType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Endereço de email do Utilizador"
                  ]
                }
              },
              {
                "@name": "languageCode",
                "@type": "languageCodeTypeEnum",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Linguagem do utilizador"
                  ]
                }
              },
              {
                "@name": "entity",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Entidade principal do utilizador"
                  ]
                },
                "xsd:complexType": {
                  "@mixed": false,
                  "xsd:sequence": {
                    "@maxOccurs": 1,
                    "@minOccurs": 1,
                    "xsd:element": {
                      "@name": "id",
                      "@type": "entityIdType",
                      "@maxOccurs": "unbounded",
                      "@minOccurs": 1,
                      "@nillable": false
                    }
                  }
                }
              },
              {
                "@name": "roleCode",
                "@type": "userRoleTypeEnum",
                "@maxOccurs": "unbounded",
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Perfil do utilizador"
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "@name": "consumerType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "Dados de consunimors para B2C"
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "documentType",
          "xsd:sequence": {
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "xsd:element": [
              {
                "@name": "consumerId",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Identificação do consumidor, utilizada para autenticação"
                  ]
                },
                "xsd:complexType": {
                  "@mixed": false,
                  "xsd:simpleContent": {
                    "xsd:extension": {
                      "@base": "userIdType",
                      "xsd:attribute": [
                        {
                          "@name": "changedConsumerId",
                          "@type": "userIdType",
                          "@use": "optional",
                          "xsd:annotation": {
                            "xsd:documentation": [
                              "Utilizar apenas quando o identificador do consumidor é alterado. Deverá ser enviado o identificador alterado neste atributo, enviando o novo identificador no elemento habitual."
                            ]
                          }
                        }
                      ]
                    }
                  }
                }
              },
              {
                "@name": "passWord",
                "@type": "userPassWordType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Senha de autenticação do utilizador"
                  ]
                }
              },
              {
                "@name": "name",
                "@type": "userNameType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Nome completo do consumidor"
                  ]
                }
              },
              {
                "@name": "addressInformation",
                "@type": "addressInformationType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Morada completa"
                  ]
                }
              },
              {
                "@name": "emailAddress",
                "@type": "emailContactType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Endereço de email do Utilizador"
                  ]
                }
              },
              {
                "@name": "languageCode",
                "@type": "languageCodeTypeEnum",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Idioma do utilizador, código ISO"
                  ]
                }
              },
              {
                "@name": "vatNumber",
                "@type": "vatNumberType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Número de indentificação fiscal (ou pessoa colectiva) da entidade."
                  ]
                }
              },
              {
                "@name": "phoneNumber",
                "@type": "phoneContactType",
                "@minOccurs": 0,
                "@maxOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Número de telefone"
                  ]
                }
              },
              {
                "@name": "consumerContract",
                "@type": "consumerContractType",
                "@maxOccurs": "unbounded",
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    {
                      "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                      "$": "Informação sobre os contractos deste consumidor"
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "@name": "UBSClientType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "UBS client type"
        ]
      },
      "xsd:sequence": {
        "@maxOccurs": 1,
        "@minOccurs": 1,
        "xsd:element": [
          {
            "@name": "biller",
            "@type": "companyInformationType",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                "Dados relatiivos à entidade que irá enviar facturas para este consumidor ao abrigo deste contrato"
              ]
            }
          },
          {
            "@name": "clientNumber",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Identificação do cliente no UBS"
                }
              ]
            },
            "xsd:simpleType": {
              "xsd:restriction": {
                "@base": "nonEmptyString",
                "xsd:maxLength": {
                  "@value": 50,
                  "@fixed": false
                }
              }
            }
          },
          {
            "@name": "accountNumber",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Conta activa do cliente"
                }
              ]
            },
            "xsd:simpleType": {
              "xsd:restriction": {
                "@base": "nonEmptyString",
                "xsd:maxLength": {
                  "@value": 50,
                  "@fixed": false
                }
              }
            }
          },
          {
            "@name": "contractLocation",
            "@type": "consumerContractLocationType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Dados relativos ao local de consumo"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "@name": "UBSConsumerRegistrationRequestType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "Pedidos de adesão provenientes do UBS, ainda não confirmados e que por isso não terão username e pwd"
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "documentType",
          "xsd:sequence": {
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "xsd:element": [
              {
                "@name": "name",
                "@type": "userNameType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Nome completo do consumidor"
                  ]
                }
              },
              {
                "@name": "addressInformation",
                "@type": "addressInformationType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Morada completa"
                  ]
                }
              },
              {
                "@name": "emailAddress",
                "@type": "emailContactType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Endereço de email do Utilizador"
                  ]
                }
              },
              {
                "@name": "languageCode",
                "@type": "languageCodeTypeEnum",
                "@minOccurs": 0,
                "@maxOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Idioma do utilizador, código ISO"
                  ]
                }
              },
              {
                "@name": "vatNumber",
                "@type": "vatNumberType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Número de indentificação fiscal (ou pessoa colectiva) da entidade."
                  ]
                }
              },
              {
                "@name": "phoneNumber",
                "@type": "phoneContactType",
                "@minOccurs": 0,
                "@maxOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Número de telefone"
                  ]
                }
              },
              {
                "@name": "consumerBillers",
                "@type": "UBSClientType",
                "@maxOccurs": "unbounded",
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    {
                      "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                      "$": "Informação sobre os Billers do consumidor"
                    }
                  ]
                }
              },
              {
                "@name": "consumerAssociation",
                "@minOccurs": 0,
                "@maxOccurs": "unbounded",
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Utilizado apenas quando é necessária a associação de contas. Por exemplo, quando há a transferências da conta entre conjuges é necessário que o novo titular, que tem nif diferente do antigo, possa ver as facturas anteriores à mudança de titularidade"
                  ]
                },
                "xsd:complexType": {
                  "@mixed": false,
                  "xsd:sequence": {
                    "@maxOccurs": 1,
                    "@minOccurs": 1,
                    "xsd:element": [
                      {
                        "@name": "vatNumber",
                        "@type": "vatNumberType",
                        "@maxOccurs": 1,
                        "@minOccurs": 1,
                        "@nillable": false,
                        "xsd:annotation": {
                          "xsd:documentation": [
                            "Número de indentificação fiscal da entidade que passará a ficar associada a esta entidade"
                          ]
                        }
                      },
                      {
                        "@name": "countryCode",
                        "@type": "countryCodeType",
                        "@maxOccurs": 1,
                        "@minOccurs": 1,
                        "@nillable": false
                      }
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    }
  ]
}