{
  "@xmlns": "urn:netdocs:schemas:document",
  "@xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
  "@xmlns:msg": "urn:netdocs:schemas:message",
  "@targetNamespace": "urn:netdocs:schemas:document",
  "@elementFormDefault": "unqualified",
  "@attributeFormDefault": "unqualified",
  "@version": "1.1",
  "@finalDefault": [],
  "@blockDefault": [],
  "xsd:annotation": [
    {
      "xsd:documentation": [
        {
          "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
          "$": "Módulo com definições de tipos de dados para documentos sem validade legal, como por exemplo a Encomenda."
        }
      ]
    }
  ],
  "xsd:include": [
    {
      "@schemaLocation": "../common/ND-BaseComponents-1.1.xsd"
    }
  ],
  "xsd:complexType": [
    {
      "@name": "orderType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          {
            "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
            "$": "Tipo de dados para suporta a ordem de compra (encomenda)."
          }
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "documentWithBaseEntitiesHeaderBodySummary",
          "xsd:attribute": [
            {
              "@name": "orderTypeCode",
              "@type": "orderTypeEnumType",
              "@use": "optional",
              "xsd:annotation": {
                "xsd:documentation": [
                  "Tipo de encomenda."
                ]
              }
            }
          ]
        }
      }
    },
    {
      "@name": "desAdvType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          {
            "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
            "$": "Tipo de dados de suporte à guia de remessa."
          }
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "desAdvWithBaseEntitiesHeaderBodySummary",
          "xsd:attribute": [
            {
              "@name": "documentType",
              "@type": "documentTypeEnumType",
              "@use": "optional",
              "xsd:annotation": {
                "xsd:documentation": [
                  "Tipo de documento a partir de onde a guia foi formada"
                ]
              }
            }
          ]
        }
      }
    },
    {
      "@name": "recAdvType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          {
            "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
            "$": "Tipo de dados de suporte ao aviso de expedição"
          }
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "recAdvWithBaseEntitiesHeaderBody",
          "xsd:attribute": [
            {
              "@name": "documentType",
              "@type": "documentTypeEnumType",
              "@use": "optional",
              "xsd:annotation": {
                "xsd:documentation": [
                  "Tipo de documento a partir de onde o aviso foi formado"
                ]
              }
            }
          ]
        }
      }
    },
    {
      "@name": "orderResponseType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          {
            "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
            "$": "Tipo de dados para suporta a resposta à ordem de compra (Resposta à Encomenda)."
          }
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "orderType",
          "xsd:annotation": {
            "xsd:documentation": [
              "A resposta à encomenda deriva da encomenda."
            ]
          }
        }
      }
    },
    {
      "@name": "documentStatusType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "Elemento representante da mensagem de estado."
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "documentType",
          "xsd:sequence": {
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "xsd:element": [
              {
                "@name": "originalDocumentReference",
                "@type": "detailedDocumentReferenceType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Dados do documento original para referênciação. Todos os elementos e atributos por debaixo deste elemento devem conter a informação do documento referênciado."
                  ]
                }
              },
              {
                "@name": "statusInformation",
                "@type": "documentStatusInformationType",
                "@maxOccurs": 1,
                "@minOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    "Informação do estado do documento original"
                  ]
                }
              },
              {
                "@name": "legalAuthorityApprovalCode",
                "@type": "xsd:string",
                "@minOccurs": 0,
                "@maxOccurs": 1,
                "@nillable": false,
                "xsd:annotation": {
                  "xsd:documentation": [
                    {
                      "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                      "$": "Código atribuído pela DGCI. No caso das guias de remessa, pode ser devolvido o código de transporte."
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "@name": "despatchAdviceType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "Elemento representante do guia de remessa"
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "desAdvType",
          "xsd:annotation": {
            "xsd:documentation": [
              "A Guia de remessa deriva em parte da estrutura fatura"
            ]
          }
        }
      }
    },
    {
      "@name": "receiptAdviceType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "Elemento representante do aviso de expedição"
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "recAdvType",
          "xsd:annotation": {
            "xsd:documentation": [
              "O aviso de expedição deriva em parte da estrutura fatura"
            ]
          }
        }
      }
    },
    {
      "@name": "priCatType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          "Elemento representante do catalogo de preços"
        ]
      },
      "xsd:complexContent": {
        "xsd:extension": {
          "@base": "priCatWithBaseEntitiesHeaderBody",
          "xsd:attribute": [
            {
              "@name": "priCatTypeCode",
              "@type": "orderTypeEnumType",
              "@use": "optional",
              "xsd:annotation": {
                "xsd:documentation": [
                  "Informação de preços"
                ]
              }
            }
          ]
        }
      }
    }
  ]
}