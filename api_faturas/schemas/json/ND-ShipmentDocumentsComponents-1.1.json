            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Global Trade Item Number (GTIN), antigamente denominado de código EAN"
                }
              ]
            }
          },
          {
            "@name": "sellerItemCode",
            "@type": "itemCodeType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Código do artigo ou serviço atribuido pelo vendedor"
                }
              ]
            }
          },
          {
            "@name": "buyerItemCode",
            "@type": "itemCodeType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Código do artigo ou serviço atribuido pelo comprador"
                }
              ]
            }
          },
          {
            "@name": "description",
            "@type": "descriptionType",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Descrição do artigo ou serviço."
                }
              ]
            }
          },
          {
            "@name": "name",
            "@type": "descriptionType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Nome do artigo ou serviço."
                }
              ]
            }
          },
          {
            "@name": "comment",
            "@type": "structuredDescriptionType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Comentário referente à linha do documento."
                }
              ]
            }
          },
          {
            "@name": "quantity",
            "@type": "quantityType",
            "@maxOccurs": 1,
            "@minOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Quantidade de artigos."
                }
              ]
            }
          },
          {
            "@name": "packQuantity",
            "@type": "quantityType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Quantidade de embalagens."
                }
              ]
            }
          },
          {
            "@name": "quantityPerPackage",
            "@type": "quantityPerPackageType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                  "$": "Quantidade contida por embalagem."
                }
              ]
            }
          },
          {
            "@name": "packQuantityPerPallet",
            "@type": "quantityType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
