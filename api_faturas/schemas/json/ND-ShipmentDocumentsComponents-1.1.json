            "@name": "sscc",
            "@type": "ssccType",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                {
                  "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
