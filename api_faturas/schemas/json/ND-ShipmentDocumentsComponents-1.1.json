              "xsd:documentation": [
                "Matrícula da viatura de transporte"
              ]
            }
          },
          {
            "@name": "transportRouteId",
            "@type": "xsd:string",
            "@minOccurs": 0,
            "@maxOccurs": 1,
            "@nillable": false,
            "xsd:annotation": {
              "xsd:documentation": [
                "Identificação da rota de transporte"
              ]
            }
          }
        ]
      }
    },
    {
      "@name": "shipmentLineItemType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          {
            "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
            "$": "Tipo de dados genérico para linhas de artigos."
          }
