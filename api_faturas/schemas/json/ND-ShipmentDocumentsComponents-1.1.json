        }
      ]
    }
  ],
  "xsd:include": [
    {
      "@schemaLocation": "../common/ND-BaseComponents-1.1.xsd"
    }
  ],
  "xsd:simpleType": {
    "@name": "shipmentDateEnumType",
    "xsd:annotation": {
      "xsd:documentation": [
        "Tipos de datas relativas a entregas"
      ]
    },
    "xsd:restriction": {
      "@base": "documentDateEnumType",
      "xsd:enumeration": [
        {
          "@value": "DELIVERY",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de entrega"
              }
            ]
          }
        },
        {
          "@value": "DELIVERYREQUESTED",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de entrega desejada"
              }
            ]
          }
        },
        {
          "@value": "DELIVERYLATEST",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora limite de entrega"
              }
            ]
          }
        },
        {
          "@value": "DEPARTURE",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de envio"
              }
            ]
          }
        },
        {
          "@value": "EXPIRATION",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de expiração"
              }
            ]
          }
        },
        {
          "@value": "ARRIVAL",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de chegada"
              }
            ]
          }
        },
        {
          "@value": "LOADDATE",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de carga"
              }
            ]
          }
        },
        {
          "@value": "UNLOADDATE",
          "xsd:annotation": {
            "xsd:documentation": [
              {
                "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
                "$": "Data e hora de descarga"
              }
            ]
          }
        }
      ]
    }
  },
  "xsd:complexType": [
    {
      "@name": "shipmentAdditionalDateType",
      "@abstract": false,
      "@mixed": false,
      "xsd:annotation": {
        "xsd:documentation": [
          {
            "@{http://www.w3.org/XML/1998/namespace}lang": "pt",
